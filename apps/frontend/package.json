{
  "name": "polly-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@navikt/aksel-icons": "^7.24.0",
    "@navikt/ds-css": "^7.24.0",
    "@navikt/ds-react": "^7.24.0",
    "@navikt/ds-tailwind": "^7.24.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "amplitude-js": "^8.0.0",
    "axios": "1.10.0",
    "baseui": "^13.0.0",
    "core-js": "^3.43.0",
    "flag-icon-css": "^4.1.7",
    "formik": "^2.4.6",
    "husky": "^9.1.7",
    "json-diff-kit": "^1.0.29",
    "lint-staged": "^16.1.2",
    "lodash": "^4.17.21",
    "moment": "2.30.1",
    "query-string": "^9.2.1",
    "randomcolor": "^0.6.2",
    "react": "^19.1.0",
    "react-app-polyfill": "^3.0.0",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-json-view": "^1.21.3",
    "react-json-view-lite": "^2.4.1",
    "react-localization": "^2.0.6",
    "react-markdown": "10.1.0",
    "react-process-string": "^1.2.0",
    "react-router": "^7.6.3",
    "react-select": "^5.10.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "shortid": "^2.2.16",
    "styletron-engine-atomic": "^1.5.0",
    "styletron-react": "^6.1.0",
    "typescript": "^5.0.2",
    "xlsx": "^0.18.0",
    "yup": "^1.1.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "format": "prettier --write \"./src\"",
    "prettier": "prettier --write \"./src\"",
    "prepare": "cd ../../ && husky apps/frontend/.husky",
    "type-check": "tsc --project tsconfig.json"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different --write",
      "eslint --max-warnings 0",
      "bash -c 'yarn run type-check'"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": [
        "warn",
        "never"
      ],
      "quotes": [
        "warn",
        "single"
      ]
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!(axios|react-day-picker|query-string|decode-uri-component|split-on-first|filter-obj)/)"
    ],
    "moduleNameMapper": {
      "^(\\.\\.?\\/.+)\\.js$": "$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^7.1.0",
    "@types/amplitude-js": "^8.0.0",
    "@types/jest": "30.0.0",
    "@types/lodash": "^4.17.19",
    "@types/node": "^24.0.3",
    "@types/randomcolor": "^0.5.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router": "^5.1.20",
    "@types/shortid": "2.2.0",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@types/styletron-standard": "^2.0.1",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "autoprefixer": "^10.4.16",
    "encoding": "^0.1.13",
    "http-proxy-middleware": "^3.0.0",
    "jest": "30.0.3",
    "jest-fixed-jsdom": "^0.0.9",
    "jest-watch-typeahead": "3.0.1",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "postcss-import": "^16.0.0",
    "prettier": "3.6.2",
    "react-scripts": "5.0.1",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2"
  }
}
